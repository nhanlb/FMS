<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd" >
    
    <absolute-ordering>
        <others />
        <name>lib</name>
    </absolute-ordering>
    
    <factory>
        <exception-handler-factory>
            com.faplib.lib.TSExceptionHandler
        </exception-handler-factory>
    </factory>
    
    <lifecycle>
        <phase-listener>
            com.faplib.admin.security.CheckUserLogin
        </phase-listener>
    </lifecycle>
    
    <application>
        <action-listener>
            com.faplib.lib.TSActionListener
        </action-listener>
    </application>
    <managed-bean>
        <managed-bean-name>centerprocess</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.CenterProcess</managed-bean-class>   
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>accesstime</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_AccessTime</managed-bean-class>   
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>address</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_Address</managed-bean-class>   
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>filepp</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_FilePP</managed-bean-class>    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>logaccess</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_LogAccess</managed-bean-class>    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>func_log_change</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_LogChange</managed-bean-class>    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>main</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_Main</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>func_menu</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_Menu</managed-bean-class>   
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>policy</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_Policy</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>func_systemgroup</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_SystemGroup</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>systemlog</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_SystemLog</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>func_mobile_module</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_MobileModule</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>    
    <managed-bean>
        <managed-bean-name>func_systemmodule</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_SystemModule</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>func_systemuser</managed-bean-name>
        <managed-bean-class>com.faplib.lib.admin.gui.FUNC_SystemUser</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>func_mng_app</managed-bean-name>
        <managed-bean-class>vn.com.telsoft.controller.MngAppsController</managed-bean-class>
    
        <managed-bean-scope>view</managed-bean-scope></managed-bean> 
    <managed-bean>
        <managed-bean-name>secuser</managed-bean-name>
        <managed-bean-class>com.faplib.admin.security.User</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cas</managed-bean-name>
        <managed-bean-class>com.faplib.admin.security.Cas</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>exception</managed-bean-name>
        <managed-bean-class>com.faplib.admin.security.ExceptionHandler</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>     
</faces-config>